{
  "name": "teleprompter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 7313",
    "build": "next build",
    "build:static": "rm -rf .next && ([ -d app/api ] && mv app/api /tmp/teleprompter-api-backup || true) && STATIC_EXPORT=true next build; ([ -d /tmp/teleprompter-api-backup ] && mv /tmp/teleprompter-api-backup app/api || true)",
    "start": "next start --port 7313",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "googleapis": "^169.0.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jsdom": "^27.4.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
